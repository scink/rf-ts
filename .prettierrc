{
	"bracketSpacing": false,
	"jsxBracketSameLine": true,
	"parser": "typescript",
	"printWidth": 120,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "all",
	"useTabs": true,
	"overrides": [
		{
			"files": ["*.json", ".eslintrc", ".prettierrc"],
			"options": {
				"parser": "json",
				"singleQuote": false,
				"trailingComma": "none"
			}
		}
	]
}
